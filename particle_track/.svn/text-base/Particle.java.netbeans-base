package particle_track;

/**
 *
 * @author tomdude
 */
public class Particle {
    
    private double x,y;
    private int elem;
    
    public Particle(double xstart, double ystart)
    {
        this.x = xstart;
        this.y = ystart;
    }
    
    public void setElem(int elem)
    {
        this.elem = elem;
    }
            
}
